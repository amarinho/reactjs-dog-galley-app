    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "webpack": "^5.27.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "hsts": "^2.2.0",
    "morgan": "^1.10.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.13.5",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.1"
  },
  "browserslist": [
    "last 2 versions",
    "not IE 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "bundles/**/*.{ts,tsx}": [
      "./node_modules/.bin/eslint -c .eslintrc.js --fix",
      "git add"
    ]
  }
